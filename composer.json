{
    "authors": [
        {
            "email": "fkooman@tuxed.net",
            "name": "Fran\u00e7ois Kooman"
        }
    ],
    "autoload": {
        "psr-4": {
            "fkooman\\OAuth\\Server\\": "src/",
            "fkooman\\OAuth\\Server\\Tests\\": "tests/"
        }
    },
    "description": "Very simple OAuth 2.0 server",
    "license": "MIT",
    "name": "fkooman/oauth2-server",
    "require": {
        "ext-date": "*",
        "ext-hash": "*",
        "ext-json": "*",
        "ext-pcre": "*",
        "ext-pdo": "*",
        "ext-spl": "*",
        "paragonie/constant_time_encoding": "^1|^2",
        "paragonie/random_compat": "^1|^2",
        "php": ">=5.4",
        "symfony/polyfill-php56": "^1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5"
    },
    "suggest": {
        "ext-libsodium": "PHP < 7.2 sodium implementation",
        "ext-sodium": "PHP >= 7.2 sodium implementation"
    },
    "type": "library"
}
